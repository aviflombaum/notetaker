<%= form_for(@reference) do |f| %>
  <div class="form-group">
    <%= f.label :name, :class => "control-label" %>
    <%= f.text_field :name, :class => "form-control", :id => "art-name"  %>
  </div>

  <div class="form-group">
    <%= f.label :reference_link, :class => "control-label" %>
    <%= f.text_field :reference_link, :class => "form-control", :id => "art-link"  %>
  </div>

  <%= f.submit "Submit", :class => "btn btn-default", :id => "submit" %>
<% end %>
