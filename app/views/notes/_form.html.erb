<%= form_for(@note) do |f| %>

  <div class="form-group">
    <%= f.collection_select(:notebook_id, Notebook.all, :id, :title) %>
  </div>

  <div class="form-group">
    <%= f.collection_select(:reference_id, Reference.all, :id, :reference_link) %>
  </div>

  <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
  </div>

  <div class="form-group">
      <%= f.label :content %>
      <%= f.text_field :content %>
  </div>

  <%= f.submit %>
<% end %>
